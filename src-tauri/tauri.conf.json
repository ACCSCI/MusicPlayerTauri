{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "test1",
  "version": "0.1.0",
  "identifier": "com.cookie.test1",
"build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "MusicBox",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "decorations": false,
        "transparent": true,
        "center": true,
        "devtools": true
      }
    ],
"security": {
      "csp": "default-src 'self' ipc: http://ipc.localhost; connect-src 'self' https://api.bilibili.com https://*.bilibili.com; img-src 'self' asset: http://asset.localhost https://*; media-src 'self' asset: http://asset.localhost blob: https://*; script-src 'self' 'unsafe-inline';",
      "assetProtocol": {
        "enable": true,
        "scope": [
          "**"
        ]
      },
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}